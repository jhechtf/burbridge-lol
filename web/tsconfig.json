{
	"extends": "astro/tsconfigs/base",
	"include": ["./astro/types.d.ts", "**/*"],
	"exclude": ["dist", "strapi"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$components/*": ["src/components/*"],
			"$layouts/*": ["src/layouts/*"],
			"$lib/*": ["src/lib/*"],
			"$types/*": ["src/types/*"]
		}
	}
}
